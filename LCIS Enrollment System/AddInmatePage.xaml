            <!--FingerPrint Capture Tab-->
            <TabItem x:Name="FingerPrintItem" BorderBrush="#FFEA7717" Foreground="Brown" Height="25" Width="100" Margin="-4,0,0,0" FontFamily="Century Gothic">
