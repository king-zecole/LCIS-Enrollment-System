                    <Button x:Name="btnReCapture" Content="Re-Capture" HorizontalAlignment="Left" Height="41" Margin="271,448,0,0" VerticalAlignment="Top" Width="127" Click="btnReCapture_Click"/>
                    <Label x:Name="ImgStatus" Content="" HorizontalAlignment="Left" Height="26" Margin="147,32,0,0" VerticalAlignment="Top" Width="511" Foreground="#FF17780E"/>

