                    <StackPanel Orientation="Horizontal">
                        <Image Height="auto" Width="auto" Source="Assest/fingerprint.gif" />
                        <TextBlock Text="FingerPrint" Margin="2,0,0,0" VerticalAlignment="Center" />
                    </StackPanel>
